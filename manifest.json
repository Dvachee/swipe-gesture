{
	"name": "Swipe Gesture",
	"version": "1.9",
	"manifest_version": 2,
	"description": "Add OSX-style multitouch gestures to Chromebooks",
	"icons": {
		"64": "img/64.png",
		"48": "img/48.png",
		"16": "img/16.png",
		"128": "img/128.png"
	},
	"options_page": "options.html",
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["swipe.js"]
		}
	],
	"permissions": [
		"storage",
		"tabs"
	]
}
